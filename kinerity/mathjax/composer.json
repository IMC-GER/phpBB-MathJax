    "name": "kinerity/mathjax",
    "type": "phpbb-extension",
    "description": "This extension makes it possible to write formulas in LaTeX notation through the integration of MathJax.",
    "keywords": ["math", "MathJax", "LaTeX", "phpBB"],
    "version": "1.2.1",
    "time": "2025-05-01",
    "license": "GPL-2.0-only",
    "authors": [
		{
			"name": "Thorsten Ahlers",
			"homepage": "https://github.com/IMC-GER",
			"role": "Developer"
		},
        {
            "name": "Kailey",
            "homepage": "https://www.techierambles.com/community/",
            "role": "Previous Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "phpbb/phpbb": ">=3.3.0,<4.0.0@dev",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "MathJax",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
        }
    }
}
